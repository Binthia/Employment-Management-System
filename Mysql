create schema java_practice;
use java_practice;

CREATE TABLE users (
    id INT AUTO_INCREMENT PRIMARY KEY,
    username VARCHAR(255) NOT NULL,
    password VARCHAR(255) NOT NULL
);
CREATE TABLE employees (
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(255) NOT NULL,
    position VARCHAR(255) NOT NULL,
    salary DOUBLE NOT NULL
);
INSERT INTO employees (name, position, salary) VALUES ('John Doe', 'Manager', 60000.0);
INSERT INTO employees (name, position, salary) VALUES ('Jane Smith', 'Developer', 50000.0);

select*from users
